{
	"name": "mcanvas",
	"version": "1.3.8",
	"description": "the image-composer or image-croper that can draw image/text/watermark or crop the image.",
	"keywords": [
		"canvas",
		"image-composite",
		"image-textview",
		"image-crop",
		"image-croper",
		"watermark",
		"cropper"
	],
	"main": "dist/mcanvas.js",
	"module": "dist/mcanvas.es.js",
	"moduleName": "MCanvas",
	"scripts": {
		"reload": "./node_modules/.bin/livereload 'example/'",
		"watch": "NODE_ENV=example rollup -w -c rollup.config.js",
		"dev": "npm-run-all --parallel reload watch",
		"es": "NODE_ENV=es rollup -c rollup.config.js",
		"pre": "NODE_ENV=development rollup -c rollup.config.js",
		"prod": "NODE_ENV=production rollup -c rollup.config.js",
		"publish": "rm -rf dist && npm-run-all --parallel es pre prod"
	},
	"repository": {
		"url": "https://github.com/xd-tayde/mcanvas",
		"type": "git"
	},
	"author": "gxd",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.4.0",
		"@babel/preset-env": "^7.4.2",
		"babel-eslint": "^7.2.2",
		"babel-plugin-external-helpers": "^6.22.0",
		"babel-plugin-syntax-flow": "^6.18.0",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-flow-strip-types": "^6.22.0",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-latest": "^6.24.1",
		"eslint": "^3.19.0",
		"expect.js": "^0.3.1",
		"livereload": "^0.7.0",
		"npm-run-all": "^4.1.5",
		"rollup": "^1.10.1",
		"rollup-plugin-babel": "^4.3.2",
		"rollup-plugin-commonjs": "^9.2.2",
		"rollup-plugin-node-resolve": "^4.0.1",
		"rollup-plugin-replace": "^2.2.0",
		"rollup-plugin-sass": "^1.2.2",
		"rollup-plugin-serve": "^1.0.1",
		"rollup-plugin-sourcemaps": "^0.4.2",
		"rollup-plugin-typescript": "^1.0.1",
		"rollup-plugin-uglify": "^6.0.2",
		"rollup-watch": "^4.3.1",
		"tslib": "^1.9.3",
		"tslint": "^5.16.0",
		"tslint-react": "^4.0.0",
		"typescript": "^3.4.5"
	},
	"dependencies": {}
}
